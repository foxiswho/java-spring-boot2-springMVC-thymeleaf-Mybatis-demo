<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.foxwho.demo.mapper.UserMapper" >
  <select id="selectUsers" resultType="com.foxwho.demo.entity.UserDomain">
      SELECT
			user_id  userId,
			user_name  userName,
			password  password,
			phone
		FROM
			t_user
  </select>
</mapper>